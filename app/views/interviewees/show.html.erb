<div style="margin: 50px;">
      <div style="margin-bottom: 0px;margin-top: 30px;">
        <div style="font-size: 28px;">
          <div style="float: left;"><strong>Click on your name if listed below or <a href="/interviewees/new">Sign Up</a></strong></div>
          <div style="float: right; font-size: 18px;"><strong>Type to Filter:</strong>  <input style="line-height: 30px;" ng-model="person"></div>
        </div>
      </div>
      <div style="clear: both;"></div>
      <div style="overflow-y: scroll; height: 600px;">
      <table class="table table-striped" style="margin-top: 50px;">
        <thead>
          <tr>
            <th>Name</th>
            <th>Email</th>
            <% if admin? && !manager? %>
              <th>SWE</th>
              <th>QA</th>
              <th>Manager QA</th>
              <th>Architect</th>
              <th>BSA</th>
              <th>UX Designer</th>
              <th>Sr. Prod Man.</th>
              <th>Proj. Man, Soft Dev</th>
              <th>Sr. BA</th>
            <% end %>
          </tr>
        </thead>
        <tbody ng-controller="IntervieweeCntrl">
            <tr ng-repeat="i in interviewees|filter:person">
              <td><a href="/interviewees/edit?interviewee={{i.id}}" ng-click="click($index)">{{i.first_name}} {{i.last_name}}</a></td>
              <td>{{ i.email }}</td>
              <% if admin? && !manager? %>
                <td><input type="checkbox" ng-change="changePosition($index, 0)" ng-model="i.selected[0]"></td>
                <td><input type="checkbox" ng-change="changePosition($index, 1)" ng-model="i.selected[1]"></td>
                <td><input type="checkbox" ng-change="changePosition($index, 2)" ng-model="i.selected[2]"></td>
                <td><input type="checkbox" ng-change="changePosition($index, 3)" ng-model="i.selected[3]"></td>
                <td><input type="checkbox" ng-change="changePosition($index, 4)" ng-model="i.selected[4]"></td>
                <td><input type="checkbox" ng-change="changePosition($index, 5)" ng-model="i.selected[5]"></td>
                <td><input type="checkbox" ng-change="changePosition($index, 6)" ng-model="i.selected[6]"></td>
                <td><input type="checkbox" ng-change="changePosition($index, 7)" ng-model="i.selected[7]"></td>
                <td><input type="checkbox" ng-change="changePosition($index, 8)" ng-model="i.selected[8]"></td>
                <td><button class="btn btn-danger" ng-click="deleteUser($index)">Delete</button></td>
              <% end %>
            </tr>
        </tbody>
      </table>
    </div>
  </div>
</div>