<div id="container" style="margin-top: 40px; margin-left: 40px;">
  <h1>Show Interviewees</h1>
  <form action="/interviewees" method="post">
    <label>First Name</label>
    <input type="text" name="interviewee[first_name]">
    <label>Last Name</label>
    <input type="text" name="interviewee[last_name">
    <label>Email</label>
    <input type="text" name="interviewee[email]">
    <label>Role</label>
    <select>
      <option>Software Engineer</option>
      <option>Business Analyst</option>
    </select>
    </br></br>
    <input class="btn" type="submit">
  </form>
</div>